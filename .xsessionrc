# Setup monitors and load wallpaper.
~/.local/bin/set-monitors && ~/.fehbg &

# Setup screenlock/saver
xset s off
xset dpms 600 900 1200
xautolock -time 5 -locker 'env XSECURELOCK_FONT=lucidasans-18 XSECURELOCK_SAVER=saver_xscreensaver xsecurelock' &

# Startup kupfer
kupfer --no-splash > /dev/null &
